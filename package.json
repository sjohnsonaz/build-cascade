  "repository": {
    "type": "git",
    "url": "https://github.com/sjohnsonaz/cascade.git"
  },
  "bugs": {},
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/mocha": "5.0.0",
    "chai": "4.1.2",
    "es6-shim": "0.35.3",
    "jsdom": "11.7.0",
    "mocha": "5.0.5",
    "reflect-metadata": "0.1.12",
    "rimraf": "2.6.2",
    "ts-loader": "4.1.0",
    "typedoc": "0.11.1",
    "typescript": "2.8.1",
    "webpack": "4.4.1",
    "webpack-cli": "2.0.13"
  },
  "keywords": [
    "cascade",
    "observable",
